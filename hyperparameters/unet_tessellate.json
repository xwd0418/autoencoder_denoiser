{
    "experiment_name": "Unet_experiment",
    "dataset": {
      "shuffle": true,
      "noise_type": "white",
      "noise_factor": "random",
      "pre-filtered": false,
      "tessellate": true,
      "batch_size": 64,
      "num_workers": 4
    },
    "experiment": {
      "num_epochs": 1,
      "learning_rate": 5e-7,
      "loss_func":"MSE",
      "lr_step": 0,
      "lr_scheduler_step": [30,80, 140, 200],
      "lr_scheduler_type": "criterion"
    },
    "model": {
        "model_type": "UNet",
        "bilinear": true
      }
  }
  